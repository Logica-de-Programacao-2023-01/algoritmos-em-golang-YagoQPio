package main

import "fmt"

func main() {
	var numero1, numero2, numero3 int
	fmt.Print("Qual é o seu numero1:")
	fmt.Scan(&numero1)

	fmt.Print("Qual é o seu numero2:")
	fmt.Scan(&numero2)

	fmt.Print("Qual é o seu numero3:")
	fmt.Scan(&numero3)

	resultado := numero1 + numero2 + numero3
	fmt.Printf("Seu resultado é", resultado)
}
