package main

import "fmt"

func main() {
	var num1, num2 int

	fmt.Print("Digite o primeiro número inteiro: ")
	fmt.Scanln(&num1)

	fmt.Print("Digite o segundo número inteiro: ")
	fmt.Scanln(&num2)

	if num1 > 0 && num2 > 0 {
		fmt.Println("O resultado da multiplicação é:", num1*num2)
	} else {
		fmt.Println("O resultado da soma é:", num1+num2)
	}
}
