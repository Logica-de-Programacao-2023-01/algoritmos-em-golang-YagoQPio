package main

import "fmt"

func main() {
	var num1, num2, num3 float64
	fmt.Print("qual é o primeiro numero?")
	fmt.Scan(&num1)
	fmt.Print("Qual é o segundo numero?")
	fmt.Scan(&num2)
	fmt.Print("qual é o terceiro numero?")
	fmt.Scan(&num3)

	if num1 < num2 && num1 < num3 {
		fmt.Println("primeiro numero é o menor")
	} else if num2 < num3 {
		fmt.Println("segundo numero é menor")
	} else {
		fmt.Println("terceiro numero é menor")
	}
}
